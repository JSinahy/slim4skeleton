{
    "name": "slim/slim4skeleton",
    "description": "A Slim Framework skeleton application for rapid development",
    "keywords": [
        "microframework",
        "rest",
        "router",
        "psr7"
    ],
    "homepage": "http://github.com/slimphp/Slim-Skeleton",
    "license": "MIT",
    "authors": [
        {
            "name": "Jose Sinahy Lara",
            "email": "jslara@proacit.com.mx",
            "homepage": "http://www.proacit.com.mx/"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "illuminate/database": "^8.25",
        "monolog/monolog": "^2.2",
        "php-di/php-di": "^6.3",
        "rbdwllr/reallysimplejwt": "^3.1",
        "slim/psr7": "^1.3",
        "slim/slim": "^4.7",
        "symfony/yaml": "^5.2",
        "zircote/swagger-php": "^3.1"
    },
    "require-dev": {
        "jangregor/phpstan-prophecy": "^0.8.1",
        "phpstan/extension-installer": "^1.1.0",
        "phpstan/phpstan": "^0.12.64",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }
}
